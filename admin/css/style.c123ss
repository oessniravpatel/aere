#semester-one .create-timeline, #semester-two .create-timeline {
	border-top: 1px solid #ccd1d8;
	width: 100%;
	line-height: 1;
	padding-bottom: 30px;
	margin: 0 auto;
	position: relative;
	display: block;
	padding-right: 20px;
	padding-left: 20px;
}
#semester-one .create-timeline span:before {
	content: "\25CB";
	color: #ccd1d8;
	top: -12px;
	display: block;
	height: 12px;
	left: -18px;
	position: absolute;
	width: 12px;
	font-size: 22px;
}
#semester-one .create-timeline span:after {
	content: "\25D0";
	color: #ccd1d8;
	top: -6px;
	display: block;
	height: 12px;
	right: -18px;
	position: absolute;
	width: 12px;
	font-size: 12px;
}
#semester-two .create-timeline.half-width span:before {
	content: "\25D0";
	color: #ccd1d8;
	top: -6px;
	display: block;
	height: 12px;
	left: -18px;
	position: absolute;
	width: 12px;
	font-size: 12px;
}
#semester-two .create-timeline.half-width span:after {
	content: "\2022";
	color: #ccd1d8;
	top: -5px;
	display: block;
	height: 12px;
	right: -18px;
	position: absolute;
	width: 12px;
	font-size: 50px;
	line-height: 0;
}
